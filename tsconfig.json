{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@jobs/*": ["./src/jobs/*"],
      "@events/*": ["./src/events/*"],
      "@commands/*": ["./src/commands/*"],
      "@modules/*": ["./src/modules/*"],
      "@util/*": ["./src/util/*"],
      "@assets/*": ["./src/assets/*"],
      "@express/*": ["./src/express/*"]
    },
    "resolveJsonModule": true
  }
}
